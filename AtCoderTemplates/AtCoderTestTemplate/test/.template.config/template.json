{
    "$schema": "http://json.schemastore.org/template",
    "author": "Kazuki Wada",
    "classifications": [
        "Test",
        "MSTest"
    ],
    "identity": "AtCoderContest.TestTemplate.CSharp",
    "name": "AtCoderContestTestTemplate",
    "shortName": "actest",
    "symbols": {
        "skipRestore": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false"
        }
    },
    "postActions": [
        {
            "condition": "(!skipRestore)",
            "description": "Restore NuGet packages required by this project.",
            "manualInstructions": [
                {
                    "text": "Run 'dotnet restore'"
                }
            ],
            "actionId": "210D431B-A78B-4D2F-B762-4ED3E3EA9025",
            "continueOnError": true
        }
    ],
    "primaryOutputs": [
        {
            "path": "test.csproj"
        }
    ]
}